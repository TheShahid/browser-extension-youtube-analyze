{
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "Browser Extension Youtube",
    "description": "Extension to get youtube video details",
  
    "icons": {
      "16": "images/16.png",
      "48": "images/48.png",
      "128": "images/128.png"
    },
  
    "browser_action": {
      "default_icon":  "images/48.png",
      "default_popup":  "index.html"
    },
  
    "permissions": ["activeTab","tabs","downloads"]
  
  }